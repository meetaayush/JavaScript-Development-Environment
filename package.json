{
  "name": "javascript-development-environment",
  "version": "0.0.1",
  "description": "Javascript development environment to build apps in Node.js",
  "main": "server.js",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage",
    "start": "npm-run-all --parallel open:src lint:watch test:watch",
    "open:src": "babel-node buildScripts/server.js",
    "localtunnel": "lt --port 5000 --subdomain ayush-dev-env",
    "share": "npm-run-all --parallel open:src localtunnel",
    "build": "webpack --mode=development",
    "lint": "esw webpack.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/meetaayush/JavaScript-Development-Environment.git"
  },
  "keywords": [
    "node.js",
    "babel",
    "lodash",
    "unit-testing",
    "mocha",
    "chai",
    "eslint",
    "express",
    "style-loader",
    "webpack",
    "chalk"
  ],
  "author": "Ayush Srivastav",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/meetaayush/JavaScript-Development-Environment/issues"
  },
  "homepage": "https://github.com/meetaayush/JavaScript-Development-Environment#readme",
  "devDependencies": {
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.3",
    "compression": "^1.7.4",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint-watch": "^5.0.1",
    "express": "^4.16.4",
    "jsdom": "^15.0.0",
    "json-schema-faker": "^0.5.0-rc16",
    "json-server": "^0.14.2",
    "localtunnel": "^1.9.1",
    "lodash": "^4.17.11",
    "mocha": "^6.1.4",
    "nock": "^10.0.6",
    "npm-run-all": "^4.1.5",
    "nsp": "^3.2.1",
    "numeral": "^2.0.6",
    "open": "^6.2.0",
    "rimraf": "^2.6.3",
    "style-loader": "^0.23.1",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.1",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-md5-hash": "0.0.6"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^2.0.1",
    "html-webpack-plugin": "^3.2.0",
    "save-dev": "^2.0.0"
  }
}
